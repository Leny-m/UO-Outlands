setvar lootbag
foreach item_type in item_types_magic
    while findtype item_type backpack as item
        getlabel item description
        if unidentified in description
            lift item 9999
            drop lootbag -1 -1 0
            wait 500
        else
            ignore item
        endif
    endwhile
endfor